		 xmlns:s="library://ns.adobe.com/flex/spark" 
		 xmlns:mx="library://ns.adobe.com/flex/mx" width="200" height="100%">
	<fx:Declarations>
		<!-- Place non-visual elements (e.g., services, value objects) here -->
		<fx:XMLList id="taskStatisticsData">
			<Node label="派遣信息">
				<Node label="未派遣"/>
				<Node label="已预约"/>
				<Node label="已派遣"/>
				<Node label="已完成"/>
				<Node label="已撤销"/>
			</Node>
		</fx:XMLList>
	</fx:Declarations>
	<mx:Tree width="100%" height="100%" labelField="@label" dataProvider="{taskStatisticsData}"/>
</s:Group>
